alias ls="lsd"
alias l="lsd -A1"
alias ll="lsd -Alh1"
alias la='lsd -A --blocks="name,group,date,size,permission" --date="relative" --group-directories-first --header --hyperlink="auto" --icon="always" --icon-theme="unicode" --permission="octal" --timesort --total-size'
alias lt='lsd -A --blocks="name,group,date,size,permission" --date="relative" --group-directories-first --header --hyperlink="auto" --icon="always" --icon-theme="unicode" --permission="octal" --timesort --total-size --tree --depth=3'
alias ip="ip --color=auto"
